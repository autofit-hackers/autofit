{
  "extends": "./tsconfig.json",
  "include": [
    "src/**/*.js",
    "src/**/*.jsx",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.html",
    "electron/**/*.js",
    "electron/**/*.jsx",
    "electron/**/*.ts",
    "electron/**/*.tsx",
    "electron/**/*.html",
    "./*.js",
    "./*.jsx",
    "./*.ts",
    "./*.tsx",
    "./*.html"
  ],
  "exclude": ["node_modules"]
}
