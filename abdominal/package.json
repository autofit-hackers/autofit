{
  "name": "abdominal",
  "version": "0.1.0",
  "productName": "abdominal",
  "private": true,
  "description": "AI coaching for weight training",
  "author": "autofit Inc. <info@autofit.co.jp>",
  "main": "dist/src/ipc/main/index.js",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prepare": "cd ../ &&  husky install abdominal/.husky",
    "lint-staged": "lint-staged",
    "lint:prettier": "prettier --check .",
    "lint:eslint": "eslint --max-warnings 0 .",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix": "run-s fix:prettier fix:eslint",
    "release": "yarn run standard-version",
    "release:patch": "standard-version --release-as patch",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npx cross-env NODE_ENV=development npx eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "npx prettier --ignore-path abdominal/.eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "npx prettier --ignore-path abdominal/.eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "npx prettier --ignore-path abdominal/.eslintignore --single-quote --write"
    ]
  },
  "engines": {
    "node": ">= 10.16.0"
  },
  "dependencies": {
    "@emotion/css": "^11.10.0",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@mediapipe/camera_utils": "^0.3.1640029074",
    "@mediapipe/control_utils": "^0.6.1629159505",
    "@mediapipe/drawing_utils": "^0.3.1620248257",
    "@mediapipe/pose": "^0.5.1635988162",
    "@mui/icons-material": "5.11.0",
    "@mui/material": "^5.10.14",
    "@techstark/opencv-js": "^4.6.0-release.1",
    "@tensorflow/tfjs": "^4.1.0",
    "aws-amplify": "^5.0.7",
    "date-utils": "^1.2.21",
    "dayjs": "^1.11.6",
    "echarts": "^5.4.1",
    "echarts-for-react": "^3.0.2",
    "electron-log": "^4.4.8",
    "fs": "^0.0.1-security",
    "mui": "^0.0.1",
    "opencv-ts": "^1.3.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-media-recorder": "^1.6.6",
    "react-player": "^2.10.1",
    "react-router-dom": "^6.3.0",
    "react-webcam": "^7.0.1",
    "three": "^0.147.0",
    "typeface-roboto": "^1.1.13",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/babel__core": "^7.1.19",
    "@types/eslint": "^8.4.5",
    "@types/file-saver": "^2.0.5",
    "@types/howler": "^2.2.7",
    "@types/prettier": "^2.7.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/streamsaver": "^2.0.1",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/three": "^0.143.1",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "@vitejs/plugin-react": "^2.0.0",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "electron": "^20.0.1",
    "electron-builder": "^23.3.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-react-devtools": "^0.5.3",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
